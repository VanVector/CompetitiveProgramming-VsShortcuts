      <Title>fr</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Code snippet for 'for' loop</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>fr</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>type</ID>
          <ToolTip>Control variable type</ToolTip>
          <Default>int</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>index</ID>
          <ToolTip>Index</ToolTip>
          <Default>i</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>max</ID>
          <ToolTip>Max length</ToolTip>
          <Default>n</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="cpp" Delimiter="$"><![CDATA[for ($type$ $index$ = 0; $index$ < $max$; $index$++) $end$]]></Code>
    </Snippet>
  </CodeSnippet>
