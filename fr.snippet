  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>frr</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Code snippet for reverse 'for' loop</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>frr</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>index</ID>
          <ToolTip>Index</ToolTip>
          <Default>i</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>max</ID>
          <ToolTip>Max length</ToolTip>
          <Default>n</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="cpp" Delimiter="$"><![CDATA[for (int $index$ = $max$ - 1; $index$ >= 0 ; $index$--) $end$]]></Code>
    </Snippet>
